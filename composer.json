{
	"name" : "cleartwo/phalcon-base",
	"type" : "library",
	"description" : "This is the base application for the Cleartwo Phalcon PHP Framework",
	"keywords" : [
		"phalcon",
		"framework",
		"cleartwo"
	],
	"homepage" : "https://phalcon.io",
	"license" : "BSD-3-Clause",
	"authors" : [{
			"name" : "Cleartwo",
			"email" : "info@cleartwo.co.uk"
		}
	],
	"require" : {
		"php" : ">=7.4",
		"ext-openssl" : "*",
		"ext-phalcon" : "~4.0.0",
		"robmorgan/phinx" : "~0.12",
 		"swiftmailer/swiftmailer" : "~6.2",
		"vlucas/phpdotenv" : "^3.6",
		"sonata-project/google-authenticator": "^2.3"
	},
	"require-dev" : {
		"phalcon/ide-stubs" : "4.0.6",
		"phpunit/phpunit" : "~9.2",
		"squizlabs/php_codesniffer" : "~3.5"
	},
	"suggest" : {
		"ext-apc" : "Needed to support caching ACL"
	},
	"autoload" : {
		"files" : [
		]
	},
	"archive" : {
		"exclude" : [
			"/.ci",
			".dockerignore",
			".travis.yml",
			"CODE_OF_CONDUCT.md",
			"CONTRIBUTING.md"
		]
	}
}
